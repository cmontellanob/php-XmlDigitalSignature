{
  "name": "cmontellanob/php-xml-digital-signature",
  "description": "Atualizacion y modificaciona la libreria marcelxyz/php-xml-digital-signature para firmado de documentox XML usando firmas digitales",
  "license": "MIT",
  "keywords": [
    "xml",
    "signature",
    "xmldsig",
    "xml-dsig",
    "xml-sig",
    "pkcs"
  ],
  "homepage": "https://github.com/cmontellanob/php-XmlDigitalSignature",
  "require": {
    "php": ">= 8.01"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "authors": [
    {
      "name": "Carlos Montellano",
      "email": "montellano.carlos@usfx.bo"
    }
  ],
  "suggest": {
    "ext-openssl": "OpenSSL extension",
    "ext-mcrypt": "MCrypt extension"
  },
  "autoload": {
    "psr-4": {
      "XmlDsig\\": "src/"
    }
  }
}
